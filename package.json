    "cytoscape-cose-bilkent": "^4.1.0",
    "env-cmd": "^10.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "keycloak-js": "10.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "randomcolor": "^0.6.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^3.0.3",
    "react-cytoscapejs": "^1.2.1",
    "react-dom": "^16.13.1",
    "react-rating-stars-component": "^2.2.0",
    "react-router-dom": "^5.2.0",
